+
+export OMP_NUM_THREADS_CY=2
+
+export APRUNCY="mpirun -l"
+
+export NWPROD=$PWD/../..
+
+export COMOUT=$DATA
+
+export NCCMP=/gpfs/dell2/emc/modeling/noscrub/George.Gayno/util/nccmp/nccmp-nc4.7.4/src/nccmp
+
+reg_dir=$PWD
+
+./C768.fv3gfs.sh
+
+cp $DATA/summary.log  $reg_dir
+
+exit
